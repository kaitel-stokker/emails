    </mj-raw>


    <mj-section background-color="#fff" padding="0 40px ">
      <mj-column vertical-align="bottom">
        
        <mj-image 
          padding="0 0 10px 0"
          src="%%=v(@product1ImageURL)=%%"
          alt="%%=v(@product1Title)=%%"
          align="center"
          border="none"
          width="220px"
          href="%%=RedirectTo(@product1URL)=%%"
        />

        <mj-text
          padding="0 0 10px 0"
          font-weight="bold"
          align="left"
          color="#000"
          font-size="16px">
            <a
              href="%%=RedirectTo(@product1URL)=%%"
              title="%%=v(@product1Title)=%%"
              style="color:#000; text-decoration:none;"
            >
            %%=v(@product1Title)=%%
            </a> 
        </mj-text>

        <mj-text
        padding="0 0 10px 0"
        color="#b71c1c">
          <span style="font-size:12px;" >StokkerPRO kliendile:</span><br />
          <span style="font-size:16px;">%%=v(@product1Price)=%%</span>
        </mj-text>

        <mj-text  padding="0 0 10px 0">
          <a
            href="%%=RedirectTo(@product1URL)=%%"
            style="color:#1A4B7A; text-decoration:none; font-weight:600;"
          >
            Tutvu tootega &gt;
          </a>
        </mj-text>
      </mj-column>

     <mj-column vertical-align="bottom">
        
        <mj-image 
          padding="0 0 10px 0"
          src="%%=v(@product1ImageURL)=%%"
          alt="%%=v(@product1Title)=%%"
          align="center"
          border="none"
          width="220px"
          href="%%=RedirectTo(@product1URL)=%%"
        />

        <mj-text
          padding="0 0 10px 0"
          font-weight="bold"
          align="left"
          color="#000"
          font-size="16px">
            <a
              href="%%=RedirectTo(@product1URL)=%%"
              title="%%=v(@product1Title)=%%"
              style="color:#000; text-decoration:none;"
            >
            %%=v(@product1Title)=%%
            </a> 
        </mj-text>

        <mj-text
        padding="0 0 10px 0"
        color="#b71c1c">
          <span style="font-size:12px;" >StokkerPRO kliendile:</span><br />
          <span style="font-size:16px;">%%=v(@product1Price)=%%</span>
        </mj-text>

        <mj-text  padding="0 0 10px 0">
          <a
            href="%%=RedirectTo(@product1URL)=%%"
            style="color:#1A4B7A; text-decoration:none; font-weight:600;"
          >
            Tutvu tootega &gt;
          </a>
        </mj-text>
      </mj-column>
    </mj-section>

            <!-- END Product block 2 -->


    <!-- Yellow button -->
    <mj-section background-color="#ffffff" padding="20px 20px 30px 20px">
      <mj-column>
        <mj-button href="LINK_SIIN">
          Vaata lähemalt
        </mj-button>
      </mj-column>
    </mj-section>

        <!-- END Yellow button -->


    <!-- Social footer -->
   <mj-section background-color="#7DB3E8" padding="50px 0">
  <mj-column>
    <mj-social
      align="center"
      mode="horizontal"
      padding="0"
      icon-size="48px"
      inner-padding="12px 12px"
    >
      <!-- LOCATION -->
      <mj-social-element
        href="https://www.stokker.ee/contacts/shops"
        src="https://image.communication.stokker.com/lib/fe2c11737364047c701378/m/1/3abb7a0b-6856-4f44-a3b0-4db3b44aa312.png"
        background-color="transparent"
      />

      <!-- CART -->
      <mj-social-element
        href="https://www.stokker.ee/e-pood"
        src="https://image.communication.stokker.com/lib/fe2c11737364047c701378/m/1/70400227-e598-4f46-b01d-4abdcb70b695.png"
        background-color="transparent"
      />

      <!-- PHONE -->
      <mj-social-element
        href="tel:(+372)6555511"
        src="https://image.communication.stokker.com/lib/fe2c11737364047c701378/m/1/a1595dea-3de1-40ea-98eb-15733a144da8.png"
        background-color="transparent"
      />

      <!-- EMAIL -->
      <mj-social-element
        href="mailto:klient@stokker.com"
